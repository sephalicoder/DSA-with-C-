/* Author: Sephali 
   Date:  2-11-25
   Description: Duplicate within K Distance in an Array
*/




#include <bits/stdc++.h>
using namespace std;


bool checkDuplicatesWithinK(vector<int> &arr, int k) {
  
    unordered_set<int> s;

  
    for (int i = 0; i < arr.size(); i++) {
      
    
        if (s.find(arr[i]) != s.end())
            return true;

       
        s.insert(arr[i]);

   
        if (i >= k)
            s.erase(arr[i - k]);
    }
    return false;
}


int main () {
    vector<int> arr = {10, 5, 3, 4, 3, 5, 6};
    if (checkDuplicatesWithinK(arr, 3))
        cout << "Yes";
    else
        cout << "No";
}
