   Date: 25-09-2025
   Description: Linked List basic operation Search
*/

#include <iostream>
using namespace std;

struct Node {
    int data;
    Node* next;
};
// Search a value in the list
bool search(Node* head, int value) {
    while (head) {
        if (head->data == value) return true;
        head = head->next;
    }
    return false;
}
