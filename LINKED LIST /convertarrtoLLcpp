#include<bits/stdc++.h>
using namespace std; 
class Node{
  public:
  int data;
  Node*next;
  public:
  Node(int data,Node*next1){
    data= data1;
    next= next1;
  }
  public: 
  Node(int data){
    data= data2;
    next= nullptr;
  }
};
Node*convertarrll(vecto<int> &arr){
  Node*head= newNode(arr[0];
  Node*mover= head;
for(i=1;i<arr.size(); i++){
  Node*temp= newNode(arr[i]);
  mover-> next= temp; 
  mover= temp; 
}
return head; 
}
int main(){
  vector<int> arr= {2,5,8,7};
  Node*head= convertArrll(arr);
  cout<<head-> data;
}

  

